<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="HardcodedText">
    <TextView
        android:id="@+id/packedChainTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/packed_chain"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
    />
    <TextView
        android:id="@+id/packedChainViewHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="111"
        android:layout_marginStart="12dp"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintTop_toBottomOf="@id/packedChainTitle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/packedChainView2"
        />
    <TextView
        android:id="@+id/packedChainView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="22222"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toTopOf="@id/packedChainViewHeader"
        app:layout_constraintStart_toEndOf="@id/packedChainViewHeader"
        app:layout_constraintEnd_toStartOf="@id/packedChainView3"
        />
    <TextView
        android:id="@+id/packedChainView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="33333333"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toTopOf="@id/packedChainViewHeader"
        app:layout_constraintStart_toEndOf="@id/packedChainView2"
        app:layout_constraintEnd_toStartOf="@id/packedChainView4"
        />
    <TextView
        android:id="@+id/packedChainView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4444"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toTopOf="@id/packedChainViewHeader"
        app:layout_constraintStart_toEndOf="@id/packedChainView3"
        app:layout_constraintEnd_toEndOf="parent"
        />

    
</androidx.constraintlayout.widget.ConstraintLayout>